<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/iv_system"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:scaleType="fitXY"
        android:src="@drawable/homepagebg" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/activity_title"
        android:background="@drawable/homepagebg">

        <ImageView
            android:id="@+id/iv_back"
            android:layout_width="@dimen/activity_title"
            android:layout_height="match_parent"
            android:padding="@dimen/activity_title_padding"
            android:src="@drawable/back" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="详情"
            android:textColor="@color/white"
            android:textSize="@dimen/title_text_size" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_shadow"
            android:orientation="vertical"
            android:padding="@dimen/padding_left_right">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_left_right2"
                android:padding="@dimen/padding_left_right">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="名称"
                    android:textColor="@color/text"
                    android:textSize="@dimen/textsizi" />

                <TextView
                    android:id="@+id/tv_crowd"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:gravity="center"
                    android:text="未知"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/textsizi" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height2"
                android:background="@color/gray" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_left_right2"
                android:padding="@dimen/padding_left_right">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="日期"
                    android:textColor="@color/text"
                    android:textSize="@dimen/textsizi" />

                <TextView
                    android:id="@+id/tv_date"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:gravity="center"
                    android:text="未知"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/textsizi" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height2"
                android:background="@color/gray" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_left_right2"
                android:padding="@dimen/padding_left_right">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="任务状态"
                    android:textColor="@color/text"
                    android:textSize="@dimen/textsizi" />

                <TextView
                    android:id="@+id/tv_state"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:gravity="center"
                    android:text="未知"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/textsizi" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height2"
                android:background="@color/gray" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_left_right2"
                android:padding="@dimen/padding_left_right">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="有效设备号数"
                    android:textColor="@color/text"
                    android:textSize="@dimen/textsizi" />

                <TextView
                    android:id="@+id/tv_effective"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:gravity="center"
                    android:text="0个"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/textsizi" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height2"
                android:background="@color/gray" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_left_right2"
                android:padding="@dimen/padding_left_right">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="已处理设备号数"
                    android:textColor="@color/text"
                    android:textSize="@dimen/textsizi" />

                <TextView
                    android:id="@+id/tv_completed"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:gravity="center"
                    android:text="0个"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/textsizi" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height2"
                android:background="@color/gray" />

            <RelativeLayout
                android:id="@+id/rl_device"
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_left_right2"
                android:padding="@dimen/padding_left_right">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="重复设备号数"
                    android:textColor="@color/text"
                    android:textSize="@dimen/textsizi" />

                <TextView
                    android:id="@+id/tv_dupDev"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:gravity="center"
                    android:text="0个"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/textsizi" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height2"
                android:background="@color/gray" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_left_right2"
                android:padding="@dimen/padding_left_right">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="新增客户数"
                    android:textColor="@color/text"
                    android:textSize="@dimen/textsizi" />

                <TextView
                    android:id="@+id/tv_addedGuest"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:gravity="center"
                    android:text="0个"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/textsizi" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height2"
                android:background="@color/gray" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_left_right2"
                android:padding="@dimen/padding_left_right">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="预计可新增客户数"
                    android:textColor="@color/text"
                    android:textSize="@dimen/textsizi" />

                <TextView
                    android:id="@+id/tv_expectedGuest"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:gravity="center"
                    android:text="0个"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/textsizi" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height2"
                android:background="@color/gray" />
        </LinearLayout>

        <include
            android:id="@+id/line_page"
            layout="@layout/layout_hides" />
    </RelativeLayout>
</LinearLayout>