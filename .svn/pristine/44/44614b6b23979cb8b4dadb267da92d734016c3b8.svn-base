                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toLeftOf="@+id/img2"
                            android:layout_toStartOf="@+id/img2"
                            android:text="不限"
                            android:textColor="@color/gray"
                            android:textSize="@dimen/textsizi" />

                        <ImageView
                            android:id="@+id/img2"
